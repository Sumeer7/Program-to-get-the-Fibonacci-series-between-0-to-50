    "def str1(str):\n",
    "    c=0\n",
    "    k=0\n",
    "    for i in str:\n",
    "        if i.isupper():\n",
    "            c+=1\n",
    "        if i.islower():\n",
    "            k+=1\n",
    "    print(\"No. of Upper case characters :\",c)\n",
    "    print(\"No. of Lower case Characters :\",k)\n",
    "str1(\"The quick Brow Fox\")"
